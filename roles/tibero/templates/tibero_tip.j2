LISTENER_PORT=8629
CONTROL_FILES="{{ tbr_data }}/control1.ctl","{{ tbr_data }}/control2.ctl"

MAX_SESSION_COUNT={{ max_session_count }}

TOTAL_SHM_SIZE={{ total_shm_size }}
